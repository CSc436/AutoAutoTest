<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
 <Appenders>
   <File name="myFile" fileName="AutoAutoTest.log">
     <PatternLayout 
     	pattern="%d{HH:mm:ss.SSS} [%t] %-5level - %msg%n"/>
   </File>
 </Appenders>
 <Loggers>
   <Root level="info">
      <AppenderRef ref="myFile"/>
    </Root>
  </Loggers>
</Configuration>

